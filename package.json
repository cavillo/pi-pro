  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "typescript": "^4.1.5"
  },
